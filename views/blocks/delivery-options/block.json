{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/refs/heads/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "myparcelnl/delivery-options-block",
  "version": "2.0.0",
  "title": "MyParcel Delivery Options",
  "category": "woocommerce",
  "description": "If used, the customer can choose delivery options during checkout.",
  "supports": {
    "html": false,
    "align": false,
    "multiple": false,
    "reusable": false
  },
  "parent": ["woocommerce/checkout-shipping-methods-block"],
  "attributes": {
    "lock": {
      "type": "object",
      "default": {
        "remove": true,
        "move": true
      }
    },
    "text": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "myparcelnl",
  "editorScript": "file:./dist/myparcelnl-delivery-options-block.js",
  "viewScript": "file:./dist/myparcelnl-delivery-options-block-frontend.js"
}
