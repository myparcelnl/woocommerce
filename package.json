  "scripts": {
    "build": "yarn build:js && yarn translations:import && yarn bundle",
    "build:js": "nx run-many --target=build",
    "build:js:dev": "nx run-many --target=build:dev",
    "build:js:dev:backend": "nx run-many --target=build --projects='backend-*'",
    "build:js:dev:frontend": "nx run-many --target=build --projects='frontend-*'",
    "bundle": "gulp",
    "test": "nx run-many --target=test --output-style=stream",
    "test:watch": "nx run-many --target=test:watch --parallel --output-style=stream",
    "translations:import": "npx -q @edielemoine/google-docs-importer --documentId 1TPE7gwG2GXtX7vlKIaskwMy0Xr4o_ir-lsedWB86xyc -o config/pdk/translations",
    "watch": "nx run-many --parallel --output-style=stream --target=build:dev -- --watch",
    "watch:backend": "nx run-many --parallel --output-style=stream --target=build:dev --projects='backend-*' -- --watch",
    "watch:frontend": "nx run-many --parallel --output-style=stream --target=build:dev --projects='frontend-*' -- --watch",
    "ws:build": "vite build",
    "ws:build:dev": "vite build --mode development",
    "ws:dev": "vite",
    "ws:test": "vitest run",
    "ws:test:watch": "vitest"
  },
