    "@types/node": "^18.13.0",
    "@types/papaparse": "^5.3.7",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-exports": "^0.8.0",
    "gettext-parser": "^6.0.0",
    "glob": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-po-sync": "^0.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tap": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-wp-pot": "^2.5.0",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.20",
    "nx": "15.7.0",
    "papaparse": "^5.3.2",
    "prettier": "^2.8.4",
    "terser": "^5.16.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vinyl-buffer": "^1.0.1",
    "vite": "^4.1.1",
    "vite-plugin-custom-tsconfig": "^1.0.4",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^0.28.5"
