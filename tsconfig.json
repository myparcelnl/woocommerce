      "@woocommerce/resource-previews": [
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/previews/index.ts",
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/previews/index.js"
      ],
      "@woocommerce/settings": [
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/settings/shared/index.ts",
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/settings/shared/index.js"
      ],
      "@woocommerce/shared-context": [
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/shared/context/index.ts",
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/shared/context/index.js"
      ],
      "@woocommerce/shared-hocs": [
        "./private/wc-types/plugins/woocommerce-blocks/assets/js/shared/hocs/index.ts",
